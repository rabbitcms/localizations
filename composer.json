{
    "name": "rabbitcms/localizations",
    "description": "Localizations support for Laravel 5.4",
    "keywords": [
        "modules",
        "laravel",
        "module",
        "rabbitcms"
    ],
    "require": {
        "php": ">=7.0.9",
        "laravel/framework": "^5.4 | ^6.0 | ^7.0",
        "symfony/translation": "^3.4 | ^4.0 | ^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kulyk Dmytro",
            "email": "lnkvisitor.ts@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RabbitCMS\\Localizations\\": "src/"
        },
        "files": [
            "src/strftime.php"
        ]
    },
    "extra": {
        "module": {
            "providers": [
                "RabbitCMS\\Localizations\\ModuleProvider"
            ]
        }
    }
}
